{
    "manifest_version": 2,
    "name": "Message Count Tracker",
    "version": "1.0",
    "description": "Counts the number of replies from ChatGPT-4.",
    "icons": {
      "128": "icon128.png"
    },
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_icon": "icon128.png",
      "default_popup": "popup.html"
    },
    "permissions": ["webRequest", "*://chat.openai.com/*"]
  }
  